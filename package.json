  "description": "React icon selector component that integrates with the Supacons Core Icon Library",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "jest && tsc --declaration --emitDeclarationOnly && tsc && cp -r src/dist/css dist/dist/css && cp -r src/dist/fonts dist/dist/fonts",
